pipeline{
    agent{
       docker{
        
       image 'cypress/included:13.6.6' 
        args '--entrypoint=""'
       }
    }
    stages{
stage('intall dependencies'){
    steps{
      sh 'CYPRESS_INSTALL_BINARY=0 npm install'
    }
}
stage('Run cypress'){
    steps{
        //sh './batchs/e2etests.sh'
        //sh './batchs/e2etests.sh'
        sh 'npx cypress run'
    }
}

    }
    
}